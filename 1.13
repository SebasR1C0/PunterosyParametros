#include <iostream>
#include <windows.h>
using namespace std;
void intercambiar(int &x, int &y) {
	int tmp = x;
	x = y;
	y = tmp;
}
int main(){
	int x,y,z;
	cout << "Ingrese el valor de x: ";
	cin >> x;
	cout << "Ingrese el valor de y: ";
	cin >> y;
	intercambiar(x, y);
	cout << "El nuevo valor de x es: " << x << endl;
	cout << "El nuevo valor de y es: " << y << endl;
	return 0;
}
