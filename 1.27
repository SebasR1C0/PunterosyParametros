#include <iostream>
using namespace std;
void primos(int n) {
	int pri = 0, abc = 0;
	while (abc <= n) {
		for (int i = 2; i <= 100000; i++) {
			for (int j = 1; j <= i; j++) {
				if (i % j == 0)pri++;
				if (pri == 2) {
					cout << i << "\t";
					abc++;
				} pri = 0;
				if (abc == n) break;
			}if (abc == n) break;
		}
	}
}
int main() {
	int n;
	cout << "El valor de n es: ";
	cin >> n;
	primos(n);
	return 0;
}
