#include <iostream>
using namespace std;
void creciente(int a, int b) {
	for (int i = a; i <= b; i++) {
		cout << i << endl;
	}
}
void decreciente(int a, int b) {
	for (int i = a; i >= b; i--) {
		cout << i << endl;
	}
}
int main() {
	int a, b;
	cout << "Ingrese el primer numero: ";
	cin >> a;
	cout << "Ingrese el segundo numero: ";
	cin >> b;
	if (a <= b) {
		creciente(a, b);
	}
	else {
		decreciente(a, b);
	}
	return 0;
}
