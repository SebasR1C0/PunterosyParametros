#include <iostream>
#include <math.h>
using namespace std;
int main() {
	for (int i = 0; i <= 10; i++) {
		if (i == 0) {
			cout << " " << "\t";
		}
		else {
			cout << i << "\t";
		}
		for (int j = 1; j <= 10; j++) {
			if (i == 0) {
				cout << j << "\t";
			}
			else { cout << i * j << "\t"; }
		}
		cout << endl;
	}
	return 0;
}
